{
  "name": "billboards",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "migration:create": "ts-node ./scripts/createMigration.ts",
    "migration:apply": "yarn supabase migration up",
    "status": "yarn supabase status",
    "db:seed:dry": "npx tsx seed.mts",
    "db:seed": "DRY=0 npx tsx seed.mts",
    "db:reset": "yarn supabase db reset && yarn db:seed",
    "db:pull": "dotenv -- cross-var yarn supabase db pull -p %SUPABASE_DB_PASSWORD%",
    "db:push": "dotenv -- cross-var yarn supabase db push -p %SUPABASE_DB_PASSWORD%",
    "types:generate:server": "npx snaplet generate && yarn ts-node ./scripts/createTsFromDB.ts",
    "types:generate:local": "npx snaplet generate && yarn ts-node ./scripts/createTsFromDB.ts local",
    "graphql:generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@mapbox/mapbox-sdk": "^0.15.3",
    "@mapbox/search-js-core": "^1.0.0-beta.18",
    "@mapbox/search-js-react": "^1.0.0-beta.18",
    "@prisma/client": "5.7.1",
    "@supabase/supabase-js": "^2.39.1",
    "classnames": "^2.3.2",
    "framer-motion": "^10.16.16",
    "graphql": "^16.8.1",
    "mapbox-gl": "^3.0.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "ts-pattern": "^5.0.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@parcel/watcher": "^2.3.0",
    "@snaplet/copycat": "0.17.3",
    "@snaplet/seed": "^0.77.2",
    "@types/google.maps": "^3.54.10",
    "@types/mapbox-gl": "^2.7.19",
    "@types/node": "^20.10.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-var": "^1.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "snaplet": "^0.77.2",
    "supabase": "^1.127.3",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
